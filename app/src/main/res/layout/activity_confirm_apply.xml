<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.homepageFunc.xiangqing.ConfirmApplyActivity">

    <include layout="@layout/layout_title_bar"/>

    <com.htmessage.cola_marketing.widget.swipyrefresh.SwipyRefreshLayout
        android:id="@+id/srl_confirm_projects"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/lv_confirm_projects"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:dividerHeight="1px" />

    </com.htmessage.cola_marketing.widget.swipyrefresh.SwipyRefreshLayout>


</LinearLayout>