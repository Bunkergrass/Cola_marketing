    <string name="jrmf_fumin_declare">本服务由富民银行提供</string>
    <string name="jrmf_http_data_fail">获得服务端数据失败，请重试</string>
    <string name="jrmf_network_error">当前网络不佳，请稍后再试</string>
    <string name="jrmf_request_exception">当前服务器繁忙请稍后再试</string>
    <string name="jrmf_send_code_time_out">银行校验时间过长，请重新获得验证码</string>
    <style name="GridPasswordView.EditText" parent="Jrmf_b_GridPasswordView.TextView">
        <item name="android:cursorVisible">false</item>
    </style>
    <style name="Jrmf_b_DialogTheme" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@drawable/jrmf_b_dialog_background</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Jrmf_b_GridPasswordView"/>
    <style name="Jrmf_b_GridPasswordView.Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>
    <style name="Jrmf_b_GridPasswordView.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="jrmf_b_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 窗口背景色 -->
        <item name="android:windowFrame">@null</item>
        <!-- Dialog的windowFrame框为无 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 窗口是否半透明——是（与第一条配合使用） -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 这个不设置的话，可能会出现边框黑线 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <!-- 窗口弹出效果 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 是否允许背景模糊 -->
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>
